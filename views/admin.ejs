<%- include('header') -%>


<div class="container container-admin">
    <div class="row">
        <h1>Admin Page &nbsp;&nbsp;</h1>
        <form action="/admin/logout" method="post">
            <button type="submit" class="btn btn-primary">Logout</button>
        </form>
    </div>


    <div class="row">
        <h2>Leaderboard</h2>
    </div>
    <div class="admin-table">
        <% for  (let i = 0; i < data.length; i++) { %>
            <div class="admin-row">
                <div class="admin-col">
                    <p> <%= data[i].email %> </p>
                </div>
                <div class="admin-col">
                    <p> <%= data[i].points %> </p>
                </div>
            </div>
        <% } %>
    </div>
</div>


<%- include('footer') -%>